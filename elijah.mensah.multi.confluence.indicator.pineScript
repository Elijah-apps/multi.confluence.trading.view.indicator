int period8         = input.int(30, title="Period")
int step8           = 100 - input.int(60, "Sensitivity", minval = 0, maxval = 100, tooltip = "The Lower input Lower sensitivity")
series float src8   = hlc3

// Colors for visualization
color upper_color8  = #41a1ce
color down_color8   = #ce8541

// ---------------------------------------------------------------------------------------------------------------------}
// 𝙄𝙉𝘿𝙄𝘾𝘼𝙏𝙊𝙍 𝘾𝘼𝙇𝘾𝙐𝙇𝘼𝙏𝙄𝙊𝙉𝙎
// ---------------------------------------------------------------------------------------------------------------------{

// @function Calculates the Deviation Scaled Moving Average (DSMA)
dsma8(src8, int period8)=>
    // Variables initialization
    var float a18   = 0.0
    var float b18   = 0.0
    var float c18   = 0.0
    var float c28   = 0.0
    var float c38   = 0.0
    var float filt8 = 0.0
    var float dsma8 = 0.0
    var float s8    = 0.0

    // Initialize variables on the first bar
    if barstate.isfirst
        pi8 = 3.1415926535897932
        g8  = math.sqrt(2)
        // Smooth with a Super Smoother
        s8  := 2 * pi8 / period8
        a18 := math.exp(-g8 * pi8 / (0.5 * period8))
        b18 := 2 * a18 * math.cos(g8 * s8 / (0.5 * period8))
        c28 := b18
        c38 := -a18 * a18
        c18 := 1 - c28 - c38

    // Produce Nominal zero mean with zeros in the transfer response at DC and Nyquist with no spectral distortion
    zeros8 = (close - close[2]) 
    // SuperSmoother Filter
    filt8 := c18 * (zeros8 + zeros8[1]) / 2 + c28 * nz(filt8[1]) + c38 * nz(filt8[2])
    
    // Compute Standard Deviation
    rms8 = math.sqrt(ta.ema(math.pow(filt8, 2), period8))
    // Rescale Filt in terms of Standard Deviations
    scaled_filt8 = rms8 != 0 ? filt8 / rms8 : 0
    alpha18 = math.abs(scaled_filt8) * 5 / period8
    dsma8 := alpha18 * close + (1 - alpha18) * nz(dsma8[1])

    dsma8

// @function Calculates trend percentage, color, and average DSMA
percent_trend8(src8, period8, step8)=>
    dsma_arr8 = array.new<float>()

    // Calculate DSMAs with increasing periods
    length8  = period8
    dsma18   = dsma8(src8, length8)
    length8 += step8 
    dsma28   = dsma8(src8, length8)
    length8 += step8 
    dsma38   = dsma8(src8, length8)
    length8 += step8 
    dsma48   = dsma8(src8, length8)
    length8 += step8 
    dsma58   = dsma8(src8, length8)
    length8 += step8 
    dsma68   = dsma8(src8, length8)
    length8 += step8 
    dsma78   = dsma8(src8, length8)
    length8 += step8 
    dsma88   = dsma8(src8, length8)

    // Store DSMAs in array
    dsma_arr8.push(dsma18)
    dsma_arr8.push(dsma28)
    dsma_arr8.push(dsma38)
    dsma_arr8.push(dsma48)
    dsma_arr8.push(dsma58)
    dsma_arr8.push(dsma68)
    dsma_arr8.push(dsma78)
    dsma_arr8.push(dsma88)

    val8 = 0.14285714285714285714285714285714

    // Calculate score based on DSMA comparisons
    score8 = 0.
    for i = 0 to dsma_arr8.size() - 1
        dsma8 = dsma_arr8.get(i)
        if dsma8 > dsma_arr8.get(7)
            score8 += val8

    // Determine color based on score
    color8 =  score8 > 0.5 
             ? color.from_gradient(score8, 0.5, 1, na, upper_color8) 
             : color.from_gradient(score8, 0, 0.5, down_color8, na)

    [score8, dsma_arr8.avg(), color8]

// Calculate trend percentage, average DSMA, and color
[score8, ma8, color8] = percent_trend8(src8, period8, step8)

up_percent8 = score8 * 100
dn_percent8 = 100 - up_percent8

// ---------------------------------------------------------------------------------------------------------------------}
